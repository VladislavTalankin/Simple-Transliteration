<Window x:Class="AAtranslitWPF.Settings"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AAtranslitWPF"
        mc:Ignorable="d"
        Title="Настройка" Height="450" Width="270" Closing="Window_Closing" WindowStartupLocation="CenterScreen" ResizeMode="NoResize">
    <Grid>
        <DataGrid AutoGenerateColumns="False" Name = "SettingsDataGrid">
            <DataGrid.Columns>
                <DataGridTextColumn Binding="{Binding RUsymbol}" Header="Русский" Width="110"/>
                <DataGridTextColumn Binding="{Binding Equalsymbol}" Header="Эквивалент" Width="110"/>
            </DataGrid.Columns>
        </DataGrid>
    </Grid>
</Window>
